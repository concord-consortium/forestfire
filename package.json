{
  "name": "wildfire-model",
  "version": "1.2.1",
  "description": "Concord Consortium starter projects",
  "main": "index.js",
  "jest": {
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "url": "https://wildfire-model.unexisting.url.com"
    },
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/cypress/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "src/utilities/test-utils.ts"
    ],
    "moduleNameMapper": {
      "\\.svg$": "<rootDir>/__mocks__/svgMock.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "scripts": {
    "start": "webpack serve --no-https",
    "build": "npm-run-all lint:build clean build:webpack",
    "build:webpack": "webpack --mode production --devtool false",
    "clean": "rimraf dist",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "lint:build": "tslint -c tslint-build.json 'src/**/*.{ts,tsx}'",
    "lint:fix": "tslint -c tslint-build.json --fix 'src/**/*.{ts,tsx}'",
    "lint:unused": "tsc --noUnusedLocals --project .",
    "test": "jest",
    "test:all": "npm-run-all build test start",
    "test:coverage": "jest --coverage",
    "test:debug": "node --nolazy --inspect-brk ./node_modules/.bin/jest --runInBand --no-cache",
    "test:watch": "jest --watch",
    "test:coverage:watch": "jest --coverage --watch",
    "test:cypress": "cypress run",
    "test:cypress:open": "cypress open",
    "test:full": "npm-run-all test test:cypress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/concord-consortium/wildfire-model.git"
  },
  "author": "Concord Consortium",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/concord-consortium/wildfire-model/issues"
  },
  "homepage": "https://github.com/concord-consortium/wildfire-model#readme",
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^5.17.1",
    "@types/chart.js": "^2.9.37",
    "@types/enzyme": "^3.10.13",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^29.5.1",
    "@types/rc-slider": "^9.3.1",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "@types/three": "^0.152.1",
    "@types/uuid": "^9.0.1",
    "autoprefixer": "^10.4.14",
    "babel-jest": "^29.5.0",
    "css-loader": "^6.8.1",
    "cypress": "8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.7",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.1",
    "identity-obj-proxy": "^3.0.0",
    "jest-environment-jsdom": "^29.5.0",
    "mini-css-extract-plugin": "^2.7.6",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^7.3.2",
    "rimraf": "^5.0.1",
    "sass-loader": "^13.3.1",
    "script-loader": "^0.7.2",
    "style-loader": "^3.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "tslint-loader": "^3.5.4",
    "tslint-react": "^5.0.0",
    "tslint-react-hooks": "^2.2.2",
    "typescript": "^5.0.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.84.1",
    "webpack-cli": "^5.1.1",
    "webpack-dev-server": "^4.15.0"
  },
  "dependencies": {
    "@concord-consortium/lara-interactive-api": "^1.9.2",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@react-three/drei": "^9.70.0",
    "@react-three/fiber": "^8.13.0",
    "@svgr/webpack": "^8.0.1",
    "babel-core": "^6.26.3",
    "chart.js": "^2.9.4",
    "chartjs-plugin-annotation": "^0.5.7",
    "chartjs-plugin-datalabels": "^0.7.0",
    "copy-webpack-plugin": "^11.0.0",
    "jest": "^29.5.0",
    "jquery": "^3.7.0",
    "mobx": "^6.9.0",
    "mobx-react": "^7.6.0",
    "patternomaly": "^1.3.2",
    "query-string": "^8.1.0",
    "rc-slider": "^10.1.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^2.9.0",
    "react-circular-input": "^0.2.4",
    "react-dom": "^18.2.0",
    "sass": "^1.62.1",
    "screenfull": "^6.0.2",
    "shutterbug": "^1.5.0",
    "three": "0.152.2",
    "wait-on": "^7.0.1"
  }
}
